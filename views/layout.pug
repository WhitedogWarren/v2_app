doctype html
html
  head
    title= title
    link(rel='stylesheet', href='./stylesheets/style.css')
    link(rel='stylesheet', href='./stylesheets/' + page + '.css')
  body
    header
        h1= title
        #titleBanner
            #navlinks
                include ./navlinks.pug
            #logThumb
                include ./logthumb.pug
    block content
  script(src="./javascripts/socket.io/client-dist/socket.io.min.js")
  script(src="./javascripts/websocketclient.js")
  script(src="./javascripts/" + page + ".js")